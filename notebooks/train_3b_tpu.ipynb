   "source": [
    "%%bash\n",
    "# Training command for 3B model\n",
    "# TPU v6e-1 has 8 cores - optimal mesh configuration\n",
    "# Option 1: Pure data parallelism (8 devices)\n",
    "# Option 2: Model parallelism (2x2x2)\n",
    "\n",
     "python -m nash_mhc.train \\\n",
     "  --dataset HuggingFaceTB/smollm-corpus \\\n",
     "  --dataset-name cosmopedia-v2 \\\n",
     "  --tokenizer HuggingFaceTB/smollm-360M \\\n",
     "  --batch-size 32 \\\n",
    "  --learning-rate 3e-4 \\\n",
     "  --total-steps 100000 \\\n",
     "  --warmup-steps 2000 \\\n",
     "  --checkpoint-dir /content/drive/MyDrive/nash-mhc-checkpoints \\\n",
     "  --checkpoint-interval 1000 \\\n",
     "  --log-interval 50 \\\n",
     "  --mesh-data 2 --mesh-fsdp 2 --mesh-tp 2 \\\n",
     "  --num-workers 4 \\\n",
     "  --worker-buffer-size 2"
    ]
  },
  {
